# ========================================
# COM_APP - Environment Variables Template
# ========================================
# Copy this file to .env and fill in your values
# NEVER commit the .env file to version control
# ========================================

# ========================================
# Firebase Configuration - Web
# ========================================
FIREBASE_WEB_API_KEY=your_firebase_web_api_key_here
FIREBASE_WEB_APP_ID=your_firebase_web_app_id_here
FIREBASE_WEB_MESSAGING_SENDER_ID=your_messaging_sender_id_here
FIREBASE_WEB_PROJECT_ID=your_project_id_here
FIREBASE_WEB_AUTH_DOMAIN=your_project_id.firebaseapp.com
FIREBASE_WEB_STORAGE_BUCKET=your_project_id.appspot.com

# ========================================
# Firebase Configuration - Android
# ========================================
FIREBASE_ANDROID_API_KEY=your_firebase_android_api_key_here
FIREBASE_ANDROID_APP_ID=your_firebase_android_app_id_here
FIREBASE_ANDROID_MESSAGING_SENDER_ID=your_messaging_sender_id_here
FIREBASE_ANDROID_PROJECT_ID=your_project_id_here
FIREBASE_ANDROID_STORAGE_BUCKET=your_project_id.appspot.com

# ========================================
# Google Apps Script APIs
# ========================================
# These are Google Apps Script deployment URLs
# You need to deploy your own scripts and get the URLs
API_ENVIAR_SOLPE=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec
API_FEM=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec
API_COSTI=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec
API_HOVIP=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec
API_COM=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec
API_USDCOP=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec
API_OE_ACTIONS=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec
API_FILE_UPLOADER=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec
API_MAIL=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec

# ========================================
# Supabase Configuration
# ========================================
# Get these values from your Supabase project settings
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_URL_NEW=https://your-other-project-ref.supabase.co
SUPABASE_ANON_KEY_NEW=your_other_supabase_anon_key_here
